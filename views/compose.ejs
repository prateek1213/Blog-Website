<link rel="stylesheet" href="css/styles.css">
<body class="compose-body">
  

<%- include("partials/header") -%>
<h1 class="para">Compose</h1>
<form action="" method="post" >
    <div class="form-group">
      <p class="para" for="exampleFormControlInput1">Titile</p>
      <input type="text" class="form-control" name="postTitle" id="exampleFormControlInput1" placeholder="">
    </div>
    <div class="form-group">
      <p class="para" for="exampleFormControlTextarea1">Post</p>
      <textarea class="form-control" id="exampleFormControlTextarea1" name="postTextArea" rows="5" cols="30" ></textarea>
    </div>
    <button type="submit" class="btn btn-outline-dark compose-button" style="margin-left: 50px;"  >Publish</button>
  </form>
<%- include("partials/footer") -%>
</body>